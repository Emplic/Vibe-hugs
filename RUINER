end)

Section:NewButton("fe emotes/animations/music script", ":)", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Emplic/FESCRIPT/main/Animations"))()
end)

Section:NewButton("btools (no one can see it becoming fe SOON)", "lmao", function()
local tool1 = Instance.new("HopperBin",game.Players.LocalPlayer.Backpack)
